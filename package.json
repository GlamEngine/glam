    "gl-mat2": "^1.0.0",
    "gl-mat3": "^1.0.0",
    "gl-mat4": "^1.1.4",
    "gl-quat": "^1.0.0",
    "gl-vec2": "^1.0.0",
    "gl-vec3": "^1.0.3",
    "jsdoc": "^3.4.0",
    "object-assign": "^4.0.1"
  },
  "dependencies": {
    "brfs": "^1.4.1",
