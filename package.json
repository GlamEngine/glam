{
  "name": "glam",
  "version": "0.0.2",
  "description": "Experimental WebGL Engine",
  "main": "index.js",
  "directories": {
    "example": "examples"
  },
  "jsnext:main": "index.js",
  "scripts": {
    "dev": "budo ./examples/hello-world/hello-world.js",
    "build:es5": "browserify index.js -o es5.js",
    "prepublish": "npm run build:es5"
  },
  "author": "Greg Tatum",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/glamjs/glam.git"
  },
  "bugs": {
    "url": "https://github.com/glamjs/glam/issues"
  },
  "homepage": "https://github.com/glamjs/glam#readme",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ],
      "brfs"
    ]
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.1.18",
    "babelify": "^7.2.0",
    "bunny": "^1.0.1",
    "geo-3d-box": "^1.0.4"
  },
  "dependencies": {
    "brfs": "^1.4.1",
    "canvas-fit": "^1.5.0",
    "gl-geometry": "^1.2.0",
    "gl-mat2": "^1.0.0",
    "gl-mat3": "^1.0.0",
    "gl-mat4": "^1.1.4",
    "gl-quat": "^1.0.0",
    "gl-shader": "^4.0.6",
    "gl-vec2": "^1.0.0",
    "gl-vec3": "^1.0.3",
    "object-assign": "^4.0.1",
    "poem-loop": "^1.1.1",
    "webgl-context": "^2.2.0"
  }
}
